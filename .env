ENVIRONMENT=prod
LOG_LEVEL=INFO

# NoneBot 驱动：反向 WS，由 Napcat 主动连接 NoneBot
HOST=0.0.0.0
PORT=8082
DRIVER=~fastapi

# Napcat Websocket 客户端填写的访问地址应当与下方端口/路径一致
# 例如：ws://127.0.0.1:8082/onebot/v11/ws
# Napcat 与 NoneBot 共用同一 token
ONEBOT_ACCESS_TOKEN=QWER1234

# 管理员 QQ 列表，可按需修改或留空
SUPERUSERS=["123456789"]
